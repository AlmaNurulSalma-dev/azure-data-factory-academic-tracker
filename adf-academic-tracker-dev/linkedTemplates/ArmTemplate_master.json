{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-academic-tracker-dev"
		},
		"ls_adls_academic_tracker_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_academic_tracker'"
		},
		"ls_adls_academic_tracker_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://acadtrackerdev001.dfs.core.windows.net/"
		},
		"ls_http_semantic_scholar_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.semanticscholar.org/graph/v1/"
		},
		"ls_keyvault_academic_tracker_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-academic-tracker-dev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-academic-tracker-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_academic_tracker_accountKey": {
						"value": "[parameters('ls_adls_academic_tracker_accountKey')]"
					},
					"ls_adls_academic_tracker_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_academic_tracker_properties_typeProperties_url')]"
					},
					"ls_http_semantic_scholar_properties_typeProperties_url": {
						"value": "[parameters('ls_http_semantic_scholar_properties_typeProperties_url')]"
					},
					"ls_keyvault_academic_tracker_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_academic_tracker_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}